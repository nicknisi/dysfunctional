---
import Head from '@/components/Head.astro';
import '@fontsource/monaspace-neon/index.css';

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en" class="h-full">
	<Head title={title} />
	<body class="m-auto h-full bg-obsidian">
		<slot />
	</body>
</html>
